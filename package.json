{
  "name": "@minimal-kit/starter-next-js",
  "author": "Minimals",
  "version": "7.2.0",
  "description": "Next Starter & JavaScript",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3033 --turbopack",
    "start": "next start -p 3033",
    "build": "next build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:print": "npx eslint --print-config eslint.config.mjs > eslint-show-config.json",
    "fm:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "fix:all": "npm run lint:fix && npm run fm:fix",
    "clean": "rm -rf node_modules .next out dist build",
    "re:dev": "yarn clean && yarn install && yarn dev",
    "re:build": "yarn clean && yarn install && yarn build",
    "re:build-npm": "npm run clean && npm install && npm run build"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource-variable/dm-sans": "^5.2.5",
    "@fontsource-variable/inter": "^5.2.5",
    "@fontsource-variable/nunito-sans": "^5.2.5",
    "@fontsource-variable/public-sans": "^5.2.5",
    "@fontsource/barlow": "^5.2.5",
    "@hookform/resolvers": "^5.0.1",
    "@iconify/react": "^6.0.0",
    "@mui/lab": "^7.0.0-beta.12",
    "@mui/material": "^7.1.0",
    "@mui/material-nextjs": "^7.1.0",
    "@mui/x-data-grid": "^8.4.0",
    "@mui/x-date-pickers": "^8.4.0",
    "@mui/x-tree-view": "^8.4.0",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "es-toolkit": "^1.38.0",
    "framer-motion": "^12.13.0",
    "minimal-shared": "^1.0.11",
    "next": "^15.3.2",
    "nprogress": "^0.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "simplebar-react": "^3.3.0",
    "stylis": "^4.3.6",
    "stylis-plugin-rtl": "^2.1.1",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@svgr/webpack": "^8.1.0",
    "eslint": "^9.27.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.2.0",
    "prettier": "^3.5.3"
  }
}
